{
  "name": "sossbox",
  "version": "1.0.1012",
  "description": "A Simple Online Storage Server.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg server.js -t node14 -o sossbox",
    "build:win": "pkg -t node14-win-x64 server.js -o sossbox.exe",
    "build:linux": "pkg -t node14-linux-x64 server.js -o sossbox",
    "build:mac": "pkg -t node14-macos-x64 server.js -o sossbox-macos"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "keywords": [
    "storage",
    "server",
    "REST",
    "API",
    "SOSS",
    "SOSSBox"
  ],
  "author": "Appurist Software <dev@appurist.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appurist/sossbox.git"
  },
  "bugs": {
    "url": "https://github.com/appurist/sossbox/issues"
  },
  "homepage": "https://github.com/appurist/sossbox#readme",
  "dependencies": {
    "fastify": "^3.1.1",
    "fastify-cors": "^4.1.0",
    "fastify-static": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "uuid-random": "^1.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "pkg": "^4.4.9"
  }
}
